{
  "dashboard": {
    "id": null,
    "title": "Ordinaut - System Overview",
    "tags": ["orchestrator", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 0.5
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Healthy"
                  },
                  "1": {
                    "text": "Degraded"
                  },
                  "2": {
                    "text": "Unhealthy"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "targets": [
          {
            "expr": "up{job=~\"orchestrator-.*\"}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Active Alerts",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow", 
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Task Execution Rate",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "targets": [
          {
            "expr": "rate(orchestrator_runs_total{status=\"success\"}[5m])",
            "legendFormat": "Successful Tasks",
            "refId": "A"
          },
          {
            "expr": "rate(orchestrator_runs_total{status=\"failed\"}[5m])",
            "legendFormat": "Failed Tasks",
            "refId": "B"
          }
        ]
      },
      {
        "id": 4,
        "title": "Queue Depth",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "orchestrator_due_work_queue_depth",
            "legendFormat": "Queue Depth",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Active Workers",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 17
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "count by() (orchestrator_active_workers > 0)",
            "legendFormat": "Active Workers",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Scheduler Lag",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 17
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "targets": [
          {
            "expr": "orchestrator_scheduler_lag_seconds",
            "legendFormat": "Scheduler Lag",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "API Response Times",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 26
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(orchestrator_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(orchestrator_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(orchestrator_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ]
      },
      {
        "id": 8,
        "title": "External Tool Call Success Rate",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 26
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "(rate(orchestrator_external_tool_calls_total{status_code=~\"2..\"}[5m]) / rate(orchestrator_external_tool_calls_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ]
      }
    ]
  }
}